{
    "name": "GO Questionnaire",
    "build": {
        "dockerfile": "Dockerfile",
    },
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.profiles.linux": {
                    "zsh": {
                        "path": "/bin/zsh"
                    }
                },
                "terminal.integrated.defaultProfile.linux": "zsh",
                "editor.formatOnSave": true
            },
            "extensions": [
                "golang.go"
            ]
        }
    },
}